<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.palap.billspliting">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"

        >
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".People" android:screenOrientation="portrait"/>
        <activity android:name=".Food" android:screenOrientation="portrait"/>
        <activity android:name=".Items" android:screenOrientation="portrait"/>
        <activity android:name=".Calc" android:screenOrientation="portrait"/>
        <activity android:name=".Result" android:screenOrientation="portrait"/>
    </application>

</manifest>